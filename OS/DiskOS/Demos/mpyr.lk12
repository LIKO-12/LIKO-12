['luacode'] = "\
--Mouse Pyrameds Demo\
--By Filippo @HomineLudens\
local xm=0 --Mouse x\
local ym=0 --Mouse y\
local t=0 --Timer\
local w=28 --Pyramed Width\
local h=28 --Pyramed Height\
\
local function pyr(x,y,w,h,cx,cy)\
 ccx=w/2+cx\
 ccy=h/2+cy\
 ccx=math.max(x,math.min(x+w,ccx))\
 ccy=math.max(y,math.min(y+h,ccy))\
 triangle(x,y,ccx,ccy,x+w,y,false,9)\
 triangle(x+w,y,ccx,ccy,x+w,y+h,false,3)\
 triangle(x,y,ccx,ccy,x,y+h,false,15)\
 triangle(x,y+h,ccx,ccy,x+w,y+h,false,16)\
end\
\
function mousemoved(x,y)\
 xm=x ym=y\
end\
\
function update(dt)\
 for x=0,192,w do\
  for y=0,128,h do\
   dx=xm-x-w/2\
   dy=ym-y-h/2\
   rad=math.atan2(dx,dy)\
   r=math.sqrt(dx^2+dy^2)\
   rx=math.min(w/2,r)\
   ry=math.min(h/2,r)\
   cx=rx*math.sin(rad)\
   cy=ry*math.cos(rad)\
   pyr(x,y,w-2,h-2,x+cx,y+cy)\
  end\
 end\
 flip() --Wait for the screen to draw.\
end\
\
clear(1)\
eventLoop()",
